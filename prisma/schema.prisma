datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Order {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  serviceId String
  title     String
  price     Int
  details   String
  createdAt DateTime @default(now())
}
